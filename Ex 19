programa
{
	
inteiro a, b, troca
	
	funcao inicio()
	{
		escreva("Digite o valor númerico da variável A: ")
		leia(a)
		escreva("Digite o valor númerico da variável B: ")
		leia(b)

		troca = a
		a = b
		b = troca 

		escreva("O novo valor de A é: " ,a ,"\n")
		escreva("O novo valor de B é: " ,b ,"\n")
	}
}
