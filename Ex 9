/*Faça um programa que imprima os 10 primeiros números inteiros maiores que 100*/
programa
{
	inclua biblioteca Util --> tempo
	inteiro num
	
	funcao inicio()
	{
		num = 100
		escreva("10 primeiros números maiores que 100\n")
		tempo.aguarde(1000)
		
		enquanto (num < 110) {
			num = num + 1
			escreva(num, "\n")
			tempo.aguarde(1000)
		}
	}
}	
